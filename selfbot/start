contextInfo: {
participant: itsme,
quotedMessage: {
extendedTextMessage: {
text: split,
}
}
}
}
exec(cmd, (err, stdout) => {
if (err) return arga.sendMessage(from, `root@Nfz.01:~ ${err}`, text, { quoted: arga })
if (stdout) {
arga.sendMessage(from, stdout, text, term)
}
})
break
